    "scripts": {
        "watch": "parallelshell \"npm run watch:js\" \"npm run watch:css\" \"npm run watch:assets\" \"npm run watch:index\" \"npm run reload\"",
        "watch:iis": "parallelshell \"npm run watch:js -- --deploy\" \"npm run watch:css -- --deploy\" \"npm run watch:assets -- --deploy\" \"npm run watch:index -- --deploy\" \"npm run reload -- --deploy\"",
        "watch:assets": "nodemon -e \".json|.gif|.png|.ico\" -w \"app/\" -x \"node build --task assets\"",
        "watch:js": "nodemon -e \".js|.html\" -w \"app/modules/\" -w \"app/app.js\" -x \"node build --task js\"",
        "watch:css": "nodemon -e less -w \"app/styles/less/\" -x \"node build --task css\"",
        "watch:index": "nodemon -e html -w \"app/index.html\" -x \"node build --task index\"",
        "reload": "node build/scripts/livereload.js"
    },
